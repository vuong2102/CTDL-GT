#include<bits/stdc++.h>
using namespace std;

int main(){
	int n;
	cin >> n; int m = 0;
	int a[n];
	for(int i = 0 ; i < n ; i++) cin >> a[i];
	vector<string> result;
	vector<int> v;
	for(int i = 0 ; i < n ; i++){
		v.push_back(a[i]);
		string s = "";
		sort(v.begin(),v.end());
		s += "Buoc "; s += to_string(m++);
		s += ": ";
		for(int i = 0 ; i < v.size() ; i++){
			s += to_string(v[i]) + " ";
		}
		result.push_back(s);
	}
	for(int i = result.size()-1 ; i >= 0 ; i--){
		cout << result[i] << endl;
	}
}
